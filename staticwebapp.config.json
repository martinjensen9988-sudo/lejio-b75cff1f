{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/images/*", "/css/*", "/js/*", "/fonts/*", "*.svg", "*.png", "*.jpg", "*.gif", "*.json", "/api/*"]
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "routes": [
    {
      "route": "/api/auth/session",
      "rewrite": "/api/AuthSession"
    },
    {
      "route": "/",
      "redirect": "/fri/",
      "statusCode": 301
    },
    {
      "route": "/fri/*",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "401": {
      "rewrite": "/index.html"
    },
    "403": {
      "rewrite": "/index.html"
    }
  }
}
